<Window x:Class="IPReport.View.MonthSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MonthSelect" Height="296" Width="402">
    <Grid >
        <Calendar Name="Calendar" SelectionMode="SingleDate" DisplayMode="Year" IsTodayHighlighted="False" 
                  DisplayModeChanged="Calendar_DisplayModeChanged"/>
    </Grid>
</Window>
